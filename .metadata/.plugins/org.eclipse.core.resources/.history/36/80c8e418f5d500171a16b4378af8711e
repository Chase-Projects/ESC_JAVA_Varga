
public class yourInitials 
{
	public static void main(String[] args)
	{
		System.out.println(" CCC  W   W V   V");
		System.out.println("C   C W   W V   V");
		System.out.println("C     W   W V   V");
		System.out.println("C     W W W V   V");
		System.out.println("C     WW  W V   V");
		System.out.println("C   C WW  W  V V");
		System.out.println(" CCC  W W  W ");
		
}
