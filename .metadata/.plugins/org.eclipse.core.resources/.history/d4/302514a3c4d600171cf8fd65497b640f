
public class YourScedule 
{
	public static void main( String[] args)
	  {
		  
		  String p1, p2, p3, p4, t1, t2, t3, t4;
		  
		  p1="Biology";
		  t1="Lawless";
		  p2="ECS";
		  t2="Mauro";
		  p3="Math";
		  t3="Farrar";
		  p4="English";
		  t4="Wilson";
		  
		  
		  System.out.println("----------------------------------");
		  System.out.println("|  " + p1 +     "    |  "  + t1 +       "   |");	
		  System.out.println("|  " + p2 +    "    |   "  + t2 +       "   |");	
		  System.out.println("|  " + p3 +    "    |   "  + t3 +       "   |");	
		  System.out.println("|  " + p4 +    "    |   "  + t4 +       "   |");	
		  System.out.println("----------------------------------");	
		
		  
		  

		  
		  
		  
	  }
}
