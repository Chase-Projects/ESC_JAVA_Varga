
public class StillUsingVariables 
{
	public static void main( String[] args)
	  {
		  
		  int yourgradeyear;
		  String yourname;
		  
		  yourgradeyear = 2021; 
		  yourname = "Chase Varga";
		  
		  
		  System.out.println(" My name is" + yourname + "and I’ll graduate in" +  yourgradeyear + ".");
		  
		  
 
		  
		  
	  }	 
}
